{
  "name": "react-apollo-decorators",
  "version": "1.0.0",
  "description": "Better decorators for Apollo and React",
  "author": "Nicolás López <nicolaslopezj@me.com>",
  "license": "MIT",
  "main": "./lib",
  "scripts": {
    "build": "babel ./src --out-dir ./lib",
    "prepublish": "yarn run test && yarn run build",
    "test": "jest",
    "lint": "standard \"src/**/*.js\"",
    "watch": "node watch.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicolaslopezj/react-apollo-decorators.git"
  },
  "keywords": [
    "react",
    "apollo",
    "decorators",
    "graphql"
  ],
  "devDependencies": {
    "apollo-client": "^1.0.0-rc.1",
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "colors": "^1.1.2",
    "eslint": "^3.17.1",
    "eslint-plugin-react": "^6.10.0",
    "jest-cli": "^19.0.2",
    "react": "^15.4.2",
    "react-apollo": "^0.13.3",
    "standard": "^9.0.1"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "underscore": "^1.8.3"
  },
  "peerDependencies": {
    "apollo-client": "^0.4.20",
    "react": ">=15.0.0",
    "react-apollo": "^0.5.9"
  },
  "jest": {
