{
  "name": "react-apollo-decorators",
  "version": "0.0.1",
  "description": "Better decorators for Apollo and React",
  "author": "Nicolás López <nicolaslopezj@me.com>",
  "license": "MIT",
  "main": "./lib",
  "scripts": {
    "build": "babel ./src --out-dir ./lib",
    "prepublish": "npm run test && npm run build",
    "test": "npm run lint && jest",
    "lint": "standard \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicolaslopezj/react-apollo-decorators.git"
  },
  "keywords": [
    "react",
    "apollo",
    "decorators",
    "graphql"
  ],
  "dependencies": {
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-jest": "^15.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "brfs": "^1.4.3",
    "browserify": "^13.0.0",
    "budo": "^8.0.3",
    "enzyme": "^2.4.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "jest-cli": "^15.1.1",
    "node-sass": "^3.4.2",
    "react": ">=15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "run-browser-babel": "^4.0.1",
    "standard": "*",
    "tape": "^4.0.0",
    "uglify-js": "^2.6.1"
  },
  "peerDependencies": {
    "react": ">=15.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib"
    ]
  },
  "standard": {
    "globals": [
      "it",
      "xit",
      "expect",
      "test",
      "jest"
    ],
    "parser": "babel-eslint"
